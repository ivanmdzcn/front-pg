{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Angular in Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/src/*",
        "webpack:///src/*": "${webRoot}/src/*",
        "webpack:///*": "*",
        "webpack:///./~/*": "${webRoot}/node_modules/*",
        "webpack:///core/*": "${webRoot}/node_modules/@angular/core/*",
        "webpack:///core.js": "${webRoot}/node_modules/@angular/core/fesm2022/core.mjs"
      },
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**",
        "**/zone-evergreen.js",
        "**/vendor.js"
      ]
    }
  ]
}
